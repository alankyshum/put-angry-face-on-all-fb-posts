{
  "manifest_version": 2,

  "name": "Facebook Auto Angry - auto on new feed",
  "description": "This is forked from Facebook Auto Angry",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "Logo.png"
  },

  "content_scripts" : [
    {
      "js": ["bower_components/jquery/dist/jquery.min.js", "targetTab.js", "targetTab.data.js"],
      "matches": [ "http://*/*", "https://*/*"]
    }
  ]
}
